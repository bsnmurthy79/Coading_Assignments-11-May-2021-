       // fibonacci series  using RECURSION MODEL
       
       /* 
       n=5
       output:
       0 1 1 2 3 5
       */


#include <iostream>
#include <bits/stdc++.h>
using namespace std;
  
int  fib(int n)
{
    if(n<=1)
    {
        return n;
    }
    else
    {
        return fib(n-1) + fib(n-2);
    }
    
}

int main()
{
    int n;
    cin>>n;
    
    for(int i=0;i<n;i++)
    {
        cout<<fib(i)<<endl;
    }
    return 0;
}






 
